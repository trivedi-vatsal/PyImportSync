{
  "_comment": "Example configuration file for PyImportSync",
  "_usage": "Specify with --config-file path/to/config.json",
  "_note": "PyImportSync now uses dynamic standard library detection automatically",
  "ignore_dirs": [
    "__pycache__",
    ".git",
    ".venv",
    "venv",
    "env",
    "node_modules",
    ".pytest_cache",
    "build",
    "dist",
    "static",
    "tests",
    "docs",
    "examples",
    "migrations"
  ],
  "requirements_file": "requirements.txt",
  "enable_cache": true,
  "cache_file": ".pyimportsync_cache.json",
  "use_pathspec": true
}
